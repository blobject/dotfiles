XTerm.termName: xterm-256color
XTerm.omitTranslation: fullscreen, popup-menu
XTerm*faceName: xft:monospace:size=21:weight=bold
XTerm.VT100.translations: #override \n\
  Ctrl <Key> minus: smaller-vt-font() \n\
  Ctrl <Key> equal: larger-vt-font() \n\
  Ctrl <Key> 0: set-vt-font(d) \n\
  Ctrl Shift <Key> c: copy-selection(CLIPBOARD) \n\
  Ctrl Shift <Key> v: insert-selection(CLIPBOARD) \n\
  <Btn1Up>: select-end(PRIMARY, CUT_BUFFER0) \n\
  <Btn2Up>: insert-selection(PRIMARY)
XTerm*bellIsUrgent: true
XTerm*boldColors: false
XTerm*background:  #b9bdc5
XTerm*foreground:  #36383f
XTerm*cursorColor: #16161d
XTerm*color0: #16161d
