fontname "Agave:bold:size=10"
color activeborder   "#3c56aa"
color inactiveborder "#92959d"
color urgencyborder  "#a32c2d"
color groupborder    "#91328c"
color ungroupborder  "#237e6f"
color font           "#16161d"
color selfont        "#3c56aa"
color menufg         "#36383f"
color menubg         "#b9bdc5"
borderwidth 20
gap 24 0 0 0
sticky yes
ignore polybar
unbind-key all
bind-key 4-1      group-only-1
bind-key 4-2      group-only-2
bind-key 4-3      group-only-3
bind-key 4-4      group-only-4
bind-key 4S-1     window-movetogroup-1
bind-key 4S-2     window-movetogroup-2
bind-key 4S-3     window-movetogroup-3
bind-key 4S-4     window-movetogroup-4
bind-key 4-h      window-move-left
bind-key 4-j      window-move-down
bind-key 4-k      window-move-up
bind-key 4-l      window-move-right
bind-key 4-h      window-resize-left
bind-key 4-j      window-resize-down
bind-key 4-k      window-resize-up
bind-key 4-l      window-resize-right
bind-key 4-h      window-snap-left
bind-key 4-j      window-snap-down
bind-key 4-k      window-snap-up
bind-key 4-l      window-snap-right
bind-key 4-y      window-snap-up-left
bind-key 4-u      window-snap-up-right
bind-key 4-b      window-snap-down-left
bind-key 4-n      window-snap-down-right
bind-key M-Tab    window-cycle
bind-key MS-Tab   window-rcycle
bind-key 4-period window-raise
bind-key 4-comma  window-lower
bind-key 4-f      window-fullscreen
bind-key 4-m      window-maximize
bind-key 4-minus  window-hmaximize
bind-key 4-equal  window-vmaximize
bind-key 4-Return menu-cmd
bind-key 4-slash  menu-window
bind-key 4S-r     restart
bind-key CM-BackSpace "0bye logout"
bind-key XF86MonBrightnessDown "0lum -5"
bind-key XF86MonBrightnessUp   "0lum +5"
bind-key 4-v                   "0vol s"
bind-key XF86AudioMute         "0vol x"
bind-key XF86AudioLowerVolume  "0vol -2"
bind-key XF86AudioRaiseVolume  "0vol +2"
bind-key XF86AudioMicMute      "0vol mic x"
bind-key 4-e          "emacs"
bind-key 4-t          "xterm"
bind-key 4-w          "qutebrowser"
bind-key 4S-w         "firefox"
bind-mouse 4-1 window-move
bind-mouse M-1 window-resize

